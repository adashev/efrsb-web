<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1" parallel="tests" thread-count="10">
    <test name="MessagePublishing" parallel="classes">
        <groups>
            <run>
                <include name="otfl"></include>
            </run>
        </groups>
        <classes>
            <class name="ru.qa.test.bankrot.tests.au.MessagePublishingTests"/>
        </classes>
    </test>
    <test name="ReportPublishing" parallel="classes">
        <groups>
            <run>
                <include name="otfl"></include>
            </run>
        </groups>
        <classes>
            <class name="ru.qa.test.bankrot.tests.au.ReportPublishingTest"/>
        </classes>
    </test>
</suite>
